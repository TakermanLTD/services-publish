{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Takerman.Dating.Server/bin/Debug/net8.0/Takerman.Dating.Server.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Takerman.Dating.Server",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart",
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/takerman.dating.client/src/views"
            }
        },
        {
            "name": "ASP.NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}